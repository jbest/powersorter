{
    "versions": {
        "config_format": "3.0"
    },
    "collection": {
        "name": "TEST",
        "prefix": "TEST",
        "catalog_number_regex": "(?P<catNum>(?P<instID>TEST)(-(?P<collID>L)-)*(?P<numerical>\\d+))"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 7,
        "log_directory_path": "/media/jbest/data/git/powersorter/",
        "output_base_path": "/media/jbest/data/git/powersorter/",
        "input_path": "/media/jbest/data/git/powersorter/input_dir/"
    },
    "file_types": {
        "web_jpg": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST"
        },
        "web_jpg_med": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(_(?P<size>med))(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST"
        },
        "web_jpg_med_orig": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(_)(?P<size>med)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST"
        },
        "web_jpg_thumb": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(_)(?P<size>thumb)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST"
        },
        "archive_dng": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(\\.)(?i)(?P<ext>dng)",
            "output_sub_path": "archive/TEST"
        },
        "ocr": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(_ocr)(\\.)(?i)(?P<ext>txt|json)",
            "output_sub_path": "web/TEST"
        }
    }
}
