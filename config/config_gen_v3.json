{
    "versions": {
        "config_format": "3.0"
    },
    "collection": {
        "name": "TEST",
        "prefix": "TEST",
        "catalog_number_regex": "(?P<catNum>(?P<instID>BRIT)(-(?P<collID>L)-)*(?P<numerical>\\d+))"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 7,
        "log_directory_path": "/corral-repl/projects/TORCH/archive/TEST/logs/",
        "output_base_path": "/corral-repl/projects/TORCH/",
        "input_path": "/corral-repl/projects/TORCH/TEST/"
    },
    "file_types": {
        "web_jpg": {
            "file_regex": "(_(?P<suffix>.+))*(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST/"
        },
        "web_jpg_med": {
            "file_regex": "(_(?P<suffix>.+))*(_)(?P<size>med)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST/"
        },
        "web_jpg_thumb": {
            "file_regex": "(_(?P<suffix>.+))*(_)(?P<size>thumb)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST/"
        },
        "archive_dng": {
            "file_regex": "(_(?P<suffix>.+))*(\\.)(?i)(?P<ext>dng)",
            "output_sub_path": "archive/TEST/"
        },
        "ocr": {
            "file_regex": "(_(?P<suffix>.+))*(_ocr)(\\.)(?i)(?P<ext>txt|json)",
            "output_sub_path": "web/TEST/"
        }
    }
}
