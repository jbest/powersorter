{
    "versions": {
        "config_format": "3.0"
    },
    "collection": {
        "name": "TTC",
        "prefix": "TTC",
        "catalog_number_regex": "(?P<catNum>(?P<instID>TTC)(-(?P<collID>[A-Z])-)?(?P<numerical>\\d+))",
    	"web_base": "/corral-repl/projects/TORCH/web/",
        "url_base": "https://web.corral.tacc.utexas.edu/torch/"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 6,
        "log_directory_path": "/corral-repl/projects/TORCH/archive/TTC/logs/",
        "output_base_path": "/corral-repl/projects/TORCH/",
        "input_path": "/corral-repl/projects/TORCH/staging/TTC/ready_to_process/"
    },
    "file_types": {
        "web_jpg": {
            "file_regex": "(_(?P<suffix>[A-Z]|[a-z]|[0-9]))?(\\.)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TTC/"
        },
        "web_jpg_med": {
            "file_regex": "(_(?P<suffix>[A-Z]|[a-z]|[0-9]))?(_)(?P<size>med)(\\.)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TTC/"
        },
        "web_jpg_thumb": {
            "file_regex": "(_(?P<suffix>[A-Z]|[a-z]|[0-9]))?(_)(?P<size>thumb)(\\.)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TTC/"
        },
        "archive_dng": {
            "file_regex": "(_(?P<suffix>[A-Z]|[a-z]|[0-9]))?(\\.)(?P<ext>dng)",
            "output_sub_path": "archive/TTC/"
        },
        "ocr": {
            "file_regex": "(_(?P<suffix>[A-Z]|[a-z]|[0-9]))?(_ocr)(\\.)(?P<ext>txt|json)",
            "output_sub_path": "web/TTC/"
        }
    }
}
