{
    "versions": {
        "config_format": "2.0"
    },
    "collection": {
        "name": "BRIT",
        "prefix": "BRIT",
        "catalog_number_regex": "(?P<catNum>(?P<instID>BRIT)(-(?P<collID>L)-)*(?P<numerical>\\d+))"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 7,
        "log_directory_path": "/media/jbest/data/git/powersorter/",
        "output_base_path": "/media/jbest/data/git/powersorter/",
        "input_path": "/media/jbest/data/git/powersorter/input_dir/"
    },
    "file_types": {
        "web_jpg": {
            "file_regex": "(_(?P<suffix>.+))*(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/"
        },
        "web_derivs": {
            "file_regex": "(_(?P<suffix>.+))*(_)(?P<size>med|thumb)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/"
        },
        "archive_dng": {
            "file_regex": "(_(?P<suffix>.+))*(\\.)(?i)(?P<ext>dng)",
            "output_sub_path": "archive/"
        },
        "ocr": {
            "file_regex": "(_(?P<suffix>.+))*(_ocr)(\\.)(?i)(?P<ext>txt|json)",
            "output_sub_path": "web/"
        }
    }
}
