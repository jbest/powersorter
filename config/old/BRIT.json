{
    "collection": {
        "name": "BRIT",
        "prefix": "BRIT"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 7,
        "log_directory_path": "/corral-repl/projects/TORCH/archive/BRIT/logs/",
        "output_base_path": "/corral-repl/projects/TORCH/",
        "input_path": "/corral-repl/projects/TORCH/tools/ingest/BRIT"
    },
    "file_types": {
        "web_jpg": {
            "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/BRIT/"
        },
        "web_derivs": {
            "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(?P<delimiter>_)(?P<size>med|thumb)(\\.)(?P<ext>.+)",
            "output_sub_path": "web/BRIT/"
        },
        "archive_dng": {
            "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(\\.)(?i)(?P<ext>DNG)",
            "output_sub_path": "archive/BRIT/"
        },
        "ocr": {
            "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(?P<delimiter>_)(?P<ocr>ocr)(\\.)(?P<ext>.+)",
            "output_sub_path": "web/BRIT/"
        }
    }
}
