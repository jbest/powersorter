{
    "collection": {
        "name": "TEST",
        "prefix": "TEST"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 7,
        "log_directory_path": "/corral-repl/projects/TORCH/archive/TEST/logs/",
        "output_base_path": "/corral-repl/projects/TORCH/",
        "input_path": "/corral-repl/projects/TORCH/TEST/"
    },
    "file_types": {
        "web_jpg": {
            "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/TEST/"
        },
        "web_jpg_med": {
            "regex": "(?P<catalog_number>(?P<prefix>BRIT)(?P<numerical>\\d+))(?P<delimiter>_)(?P<size>med)(\\.)(?P<ext>.+)",
            "output_sub_path": "web/TEST/"
        },
        "web_jpg_thumb": {
            "regex": "(?P<catalog_number>(?P<prefix>BRIT)(?P<numerical>\\d+))(?P<delimiter>_)(?P<size>thumb)(\\.)(?P<ext>.+)",
            "output_sub_path": "web/TEST/"
        },
        "archive_dng": {
            "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(\\.)(?i)(?P<ext>DNG)",
            "output_sub_path": "archive/TEST/"
        },
        "ocr": {
            "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(?P<delimiter>_)(?P<ocr>ocr)(\\.)(?P<ext>.+)",
            "output_sub_path": "web/TEST/"
        }
    }
}
