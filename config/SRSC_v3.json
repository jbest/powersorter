{
    "versions": {
        "config_format": "3.0"
    },
    "collection": {
        "name": "LLC",
        "prefix": "LLC",
        "catalog_number_regex": "(?P<catNum>(?P<instID>LLC)*(?P<numerical>\\d+))",
        "web_base": "/corral-repl/projects/TORCH/web/LLC",
        "url_base": "https://web.corral.tacc.utexas.edu/torch/"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 8,
        "log_directory_path": "/corral-repl/projects/TORCH/archive/LLC/logs/",
        "output_base_path": "/corral-repl/projects/TORCH/",
        "input_path": "/corral-repl/projects/TORCH/staging/LLC/"
    },
    "file_types": {
        "web_jpg": {
            "file_regex": "(_(?i)(?P<suffix>[a-z]))*(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/LLC/"
        },
        "web_jpg_med": {
            "file_regex": "(_(?i)(?P<suffix>[a-z]))*(_)(?P<size>med)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/LLC/"
        },
        "web_jpg_thumb": {
            "file_regex": "(_(?i)(?P<suffix>[a-z]))*(_)(?P<size>thumb)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/LLC/"
        },
        "archive_dng": {
            "file_regex": "(_(?i)(?P<suffix>[a-z]))*(\\.)(?i)(?P<ext>dng)",
            "output_sub_path": "archive/LLC/"
        },
        "ocr": {
            "file_regex": "(_(?i)(?P<suffix>[a-z]))*(_ocr)(\\.)(?i)(?P<ext>txt|json)",
            "output_sub_path": "web/LLC/"
        }
    }
}
