{
    "versions": {
        "config_format": "3.0"
    },
    "collection": {
        "name": "BRIT",
        "prefix": "BRIT",
        "catalog_number_regex": "(?P<catNum>(?P<instID>BRIT)(-(?P<collID>XX)-)*(?P<numerical>\\d+))",
        "web_base": "/media/jbest/data21/BRIT_git/powersorter/web",
        "url_base": "https://web.corral.tacc.utexas.edu/torch/"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 7,
        "log_directory_path": "/media/jbest/data21/BRIT_git/powersorter/",
        "output_base_path": "/media/jbest/data21/BRIT_git/powersorter",
        "input_path": "/media/jbest/data21/BRIT_git/powersorter/staging/"
    },
    "file_types": {
        "web_jpg": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/BRIT/"
        },
        "web_jpg_med": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(_)(?P<size>med)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/BRIT/"
        },
        "web_jpg_thumb": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(_)(?P<size>thumb)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/BRIT/"
        },
        "archive_dng": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(\\.)(?i)(?P<ext>dng)",
            "output_sub_path": "archive/BRIT/"
        },
        "ocr": {
            "file_regex": "(_(?P<suffix>a-z|0-9))*(_ocr)(\\.)(?i)(?P<ext>txt|json)",
            "output_sub_path": "web/BRIT/"
        }
    }
}
